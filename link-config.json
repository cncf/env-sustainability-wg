{
  "aliveStatusCodes": [200, 206, 402],
  "fallbackRetryDelay": "5s",
  "ignorePatterns": [
    {
      "pattern": "^(http|https):\/\/(www\\.)?(linkedin|twitter)"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/images/",
      "replacement": "/website/static/images/"
    },
    {
      "pattern": "^/landscape/",
      "replacement": "/website/content/landscape/_index.md"
    },
    {
      "pattern": "^/landscape-ko/",
      "replacement": "/website/content/landscape/SustainabilityUseCasesAndLandscape2023.ko.md"
    },
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "retryCount": 3,
  "retryOn429": true,
  "timeout": "20s"
}
