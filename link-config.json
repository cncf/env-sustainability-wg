{
  "aliveStatusCodes": [200, 206, 402],
  "fallbackRetryDelay": "5s",
  "ignorePatterns": [
    {
      "pattern": "^https://(twitter|linkedin)"
    },
    {
      "pattern": "^file:///github/workspace/(images|landscape|landscape-ko)/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "retryCount": 3,
  "retryOn429": true,
  "timeout": "20s"
}
